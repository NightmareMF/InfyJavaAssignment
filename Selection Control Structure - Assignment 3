class Tester{
	public static void main(String[] args) {
	int product;
	int a = 2, b = 6, c = 7;
	if(c == 7) {
		System.out.println(product = -1);
		}
	else if(b ==7) {
		System.out.println(product = c);
		}
	else if(a == 7) {
		System.out.println(product = b * c);
		}
	else
		System.out.println(product = a * b * c);
	}
}
